{
  "name": "assignment-checker",
  "displayName": "Phero Assignment checker",
  "version": "0.2.2",
  "description": "Assignment checker extensin for Programming Hero instructors by @shahriyardx",
  "author": "Md Shahriyar Alam <contact@shahriyar.dev>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --zip",
    "test": "plasmo test"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.483.0",
    "moment": "^2.30.1",
    "plasmo": "0.90.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/chrome": "^0.0.310",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.12",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.8.2"
  },
  "manifest": {
    "host_permissions": [
      "https://web.programming-hero.com/instructor-dashboard*"
    ],
    "permissions": [
      "notifications",
      "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/icon.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "key": ""
  }
}
