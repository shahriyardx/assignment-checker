{
  "name": "assignment-checker",
  "displayName": "Phero Assignment checker",
  "version": "0.2.2",
  "description": "Assignment checker extensin for Programming Hero instructors",
  "author": "Md Shahriyar Alam <contact@shahriyar.dev>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --zip",
    "test": "plasmo test"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.9.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.298.0",
    "moment": "^2.29.4",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/lodash": "^4.17.16",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.6",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://web.programming-hero.com/*"
    ],
    "permissions": [
      "notifications",
      "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/icon.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "key": ""
  }
}
